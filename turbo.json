{
  "$schema": "https://turborepo.org/schema.json",
  "globalPassThroughEnv": ["DATABASE_URL"],
  "tasks": {
    "build": {
      "dependsOn": [
        "^generate",
        "^build"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "generate": {
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
