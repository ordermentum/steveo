
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WorkflowState {
  id                Int         @id @default(autoincrement())
  flowId            String      @map("flow_id") @unique
  started           DateTime    @db.Timestamptz(6)
  current           String
  initial           Json
  results           Json?
  errors            Json?
}

