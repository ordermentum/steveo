{
  "name": "steveo",
  "version": "7.0.3",
  "description": "A Task Pub/Sub Background processing library",
  "main": "lib/index.js",
  "author": "engineering@ordermentum.com",
  "private": false,
  "license": "Apache-2.0",
  "repository": "git@github.com:ordermentum/steveo.git",
  "scripts": {
    "test": "NODE_ENV=test nyc yarn spec",
    "spec": "tsc && yarn mocha",
    "lint": "yarn eslint 'src/**/*.{ts,js}'",
    "lint:test": "yarn eslint 'test/**/*.{ts,js}'",
    "autotest": "yarn run mocha --watch",
    "prepublish": "yarn run build",
    "build": "yarn run tsc",
    "typecheck": "yarn run tsc --noEmit",
    "steveo": "node ./lib/cli-task.js"
  },
  "files": [
    "lib/*"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "exclude": [
      "coverage",
      "examples",
      "test",
      "lib",
      "src/types"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.637.0",
    "@smithy/node-http-handler": "^3.1.4",
    "@types/bluebird": "^3.5.38",
    "bluebird": "^3.7.2",
    "generic-pool": "^3.9.0",
    "lazy-object": "^1.0.1",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.merge": "4.6.2",
    "lodash.shuffle": "^4.2.0",
    "moment": "2.30.1",
    "node-rdkafka": "2.18.0",
    "null-logger": "^2.0.0",
    "rsmq": "^0.12.4",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@emotion/eslint-plugin": "11.11",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@ordermentum/eslint-config-ordermentum": "2.4.0",
    "@salesforce/ts-sinon": "^1.4.27",
    "@types/chai": "4.3.16",
    "@types/generic-pool": "3.8.3",
    "@types/lodash.merge": "4.6.9",
    "@types/mocha": "10.0.1",
    "@types/newrelic": "9.14.4",
    "@types/node": "20.14.9",
    "@types/rsmq": "0.8.4",
    "@types/sinon": "10.0.20",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "chai": "4.4.1",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-mocha": "10.4.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "45.0.2",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "pino": "^8.14.1",
    "prettier": "2.8.8",
    "rsmq": "^0.12.4",
    "sinon": "15.2.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.2",
    "typescript": "5.1.3"
  }
