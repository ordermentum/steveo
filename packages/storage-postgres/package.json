{
  "name": "@steveojs/storage-postgres",
  "version": "9.0.0",
  "description": "Postgres persistence implementation for Steveo",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "engineering@ordermentum.com",
  "private": false,
  "license": "Apache-2.0",
  "repository": "git@github.com:ordermentum/steveo.git",
  "scripts": {
    "test": "NODE_ENV=test nyc yarn spec",
    "spec": "tsc && yarn mocha",
    "lint": "yarn eslint 'src/**/*.{ts,js}'",
    "lint:test": "yarn eslint 'test/**/*.{ts,js}'",
    "autotest": "yarn run mocha --watch",
    "prepublish": "yarn run build",
    "build": "yarn run tsc",
    "generate": "npx prisma generate",
    "typecheck": "yarn run tsc --noEmit",
    "steveo": "node ./lib/cli-task.js"
  },
  "files": [
    "lib/*"
  ],
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "exclude": [
      "coverage",
      "examples",
      "test",
      "lib"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  },
  "dependencies": {
    "@prisma/client": "5.19.1",
    "@types/bluebird": "^3.5.38",
    "aws-sdk": "^2.1043.0",
    "bluebird": "^3.7.2",
    "generic-pool": "^3.9.0",
    "lazy-object": "^1.0.1",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.merge": "4.6.2",
    "lodash.shuffle": "^4.2.0",
    "moment": "2.30.1",
    "node-rdkafka": "3.3.0",
    "null-logger": "^2.0.0",
    "prisma": "^5.19.1",
    "radash": "^12.1.0",
    "redlock": "^5.0.0-beta.2",
    "rsmq": "^0.12.4",
    "uuid": "10.0.0"
  },
  "peerDependencies": {
    "steveo": "*"
  },
  "devDependencies": {
    "steveo": "*",
    "@emotion/eslint-plugin": "11.12",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/chai": "4.3.20",
    "@types/generic-pool": "3.8.3",
    "@types/lodash.merge": "4.6.9",
    "@types/mocha": "10.0.1",
    "@types/newrelic": "9.14.8",
    "@types/node": "20.17.27",
    "@types/rsmq": "0.8.4",
    "@types/sinon": "10.0.20",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "aws-sdk": "^2.1043.0",
    "chai": "4.5.0",
    "cross-env": "7.0.3",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "pino": "^8.14.1",
    "prettier": "2.8.8",
    "rsmq": "^0.12.4",
    "sinon": "15.2.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.2",
    "typescript": "5.1.3"
  }
}
