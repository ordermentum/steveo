{
  "name": "steveo",
  "version": "2.0.8",
  "description": "A Task Pub/Sub Background processing library",
  "main": "lib/index.js",
  "author": "engineering@ordermentum.com",
  "license": "Apache-2.0",
  "repository": "git@github.com:ordermentum/steveo.git",
  "scripts": {
    "test": "NODE_ENV=test nyc npm run spec",
    "spec": "yarn run mocha --recursive test/**/*",
    "lint": "yarn eslint 'src/**/*.{ts,js}'",
    "lint:test": "yarn eslint 'test/**/*.{ts,js}'",
    "autotest": "yarn run mocha --watch",
    "build:babel": "yarn typecheck && yarn run babel -Dd lib src --extensions \".js,.ts\"",
    "prepublish": "yarn run build",
    "build:ts": "yarn run tsc",
    "build": "yarn build:ts",
    "typecheck": "yarn run tsc --noEmit",
    "steveo": "node ./lib/cli-task.js",
    "flow:coverage": "yarn run flow-coverage-report -i 'src/**/*.js' -x 'src/test/**' -t html -t json -t text"
  },
  "files": [
    "lib/*"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "exclude": [
      "coverage",
      "example",
      "test",
      "lib"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "generic-pool": "^3.7.1",
    "lazy-object": "^1.0.1",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.shuffle": "^4.2.0",
    "moment": "2.24.0",
    "null-logger": "^1.0.0",
    "uuid": "^3.1.0"
  },
  "optionalDependencies": {
    "aws-sdk": "^2.473.0",
    "no-kafka": "^3.1.10",
    "rsmq": "^0.12.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-flow-strip-types": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-flow": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@babel/register": "7.9.0",
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@types/chai": "4.2.11",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.30",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-istanbul": "5.2.0",
    "chai": "4.2.0",
    "cross-env": "6.0.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-ordermentum": "0.1.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-unicorn": "14.0.1",
    "flow-bin": "0.121.0",
    "flow-coverage-report": "0.6.1",
    "mocha": "6.2.2",
    "nyc": "14.1.1",
    "prettier": "1.19.1",
    "sinon": "7.5.0",
    "source-map-support": "0.5.16",
    "typescript": "3.8.3",
    "typescript-eslint-parser": "22.0.0"
  }
}
