{
  "name": "steveo",
  "version": "3.0.3",
  "description": "A Task Pub/Sub Background processing library",
  "main": "lib/index.js",
  "author": "engineering@ordermentum.com",
  "license": "Apache-2.0",
  "repository": "git@github.com:ordermentum/steveo.git",
  "scripts": {
    "test": "NODE_ENV=test nyc yarn spec",
    "spec": "tsc && mocha test/**/*_test.ts",
    "spec:runner": "mocha",
    "lint": "yarn eslint 'src/**/*.{ts,js}'",
    "lint:test": "yarn eslint 'test/**/*.{ts,js}'",
    "autotest": "yarn run mocha --watch",
    "prepublish": "yarn run build",
    "build": "yarn run tsc",
    "typecheck": "yarn run tsc --noEmit",
    "steveo": "node ./lib/cli-task.js"
  },
  "files": [
    "lib/*"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "exclude": [
      "coverage",
      "example",
      "test",
      "lib"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "dependencies": {
    "@types/generic-pool": "^3.1.9",
    "bluebird": "^3.7.2",
    "generic-pool": "^3.7.2",
    "lazy-object": "^1.0.1",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.merge": "4.6.2",
    "lodash.shuffle": "^4.2.0",
    "moment": "2.29.1",
    "null-logger": "^2.0.0",
    "uuid": "^8.0.0"
  },
  "optionalDependencies": {
    "aws-sdk": "^2.489.0",
    "node-rdkafka": "^2.11.0",
    "rsmq": "^0.12.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/chai": "4.2.22",
    "@types/lodash.merge": "4.6.6",
    "@types/mocha": "9.0.0",
    "@types/node": "14.18.0",
    "@types/rsmq": "0.8.4",
    "@types/sinon": "10.0.6",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-istanbul": "5.2.0",
    "chai": "4.3.4",
    "cross-env": "7.0.3",
    "eslint": "6.8.0",
    "eslint-config-ordermentum": "1.0.6",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-unicorn": "35.0.0",
    "mocha": "9.1.3",
    "nyc": "15.1.0",
    "prettier": "1.19.1",
    "sinon": "12.0.1",
    "source-map-support": "0.5.21",
    "ts-node": "10.4.0",
    "typescript": "4.5.2",
    "typescript-eslint-parser": "22.0.0"
  }
}
