{
  "name": "steveo",
  "version": "5.1.0-beta",
  "description": "A Task Pub/Sub Background processing library",
  "main": "lib/index.js",
  "author": "engineering@ordermentum.com",
  "license": "Apache-2.0",
  "repository": "git@github.com:ordermentum/steveo.git",
  "scripts": {
    "test": "NODE_ENV=test nyc yarn spec",
    "spec": "tsc && yarn spec:runner 'test/**/*_test.ts'",
    "spec:runner": "yarn mocha",
    "lint": "yarn eslint 'src/**/*.{ts,js}'",
    "lint:test": "yarn eslint 'test/**/*.{ts,js}'",
    "autotest": "yarn run mocha --watch",
    "prepublish": "yarn run build",
    "build": "yarn run tsc",
    "typecheck": "yarn run tsc --noEmit",
    "steveo": "node ./lib/cli-task.js"
  },
  "files": [
    "lib/*"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "exclude": [
      "coverage",
      "examples",
      "test",
      "lib"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  },
  "dependencies": {
    "@types/bluebird": "^3.5.38",
    "bluebird": "^3.7.2",
    "eslint": "^8.30.0",
    "generic-pool": "^3.8.2",
    "lazy-object": "^1.0.1",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.merge": "4.6.2",
    "lodash.shuffle": "^4.2.0",
    "moment": "2.29.4",
    "null-logger": "^2.0.0",
    "uuid": "^8.0.0"
  },
  "optionalDependencies": {
    "aws-sdk": "^2.1043.0",
    "node-rdkafka": "^2.13.0",
    "rsmq": "^0.12.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/chai": "4.3.4",
    "@types/generic-pool": "3.1.11",
    "@types/lodash.merge": "4.6.7",
    "@types/mocha": "10.0.1",
    "@types/newrelic": "^9.4.0",
    "@types/node": "16.18.3",
    "@types/rsmq": "0.8.4",
    "@types/sinon": "10.0.13",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-istanbul": "5.2.0",
    "chai": "4.3.7",
    "cross-env": "7.0.3",
    "eslint-config-ordermentum": "1.0.6",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-unicorn": "35.0.0",
    "mocha": "^10.1.0",
    "nyc": "15.1.0",
    "prettier": "1.19.1",
    "sinon": "15.0.1",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "typescript-eslint-parser": "22.0.0"
  }
}
