{
  "name": "steveo",
  "version": "2.3.2",
  "description": "A Task Pub/Sub Background processing library",
  "main": "lib/index.js",
  "author": "engineering@ordermentum.com",
  "license": "Apache-2.0",
  "repository": "git@github.com:ordermentum/steveo.git",
  "scripts": {
    "test": "NODE_ENV=test nyc yarn spec",
    "spec": "tsc && mocha test/**/*_test.ts",
    "lint": "yarn eslint 'src/**/*.{ts,js}'",
    "lint:test": "yarn eslint 'test/**/*.{ts,js}'",
    "autotest": "yarn run mocha --watch",
    "prepublish": "yarn run build",
    "build": "yarn run tsc",
    "typecheck": "yarn run tsc --noEmit",
    "steveo": "node ./lib/cli-task.js"
  },
  "files": [
    "lib/*"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "exclude": [
      "coverage",
      "example",
      "test",
      "lib"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "generic-pool": "^3.7.2",
    "lazy-object": "^1.0.1",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.shuffle": "^4.2.0",
    "moment": "2.29.1",
    "null-logger": "^1.0.0",
    "uuid": "^3.1.0"
  },
  "optionalDependencies": {
    "aws-sdk": "^2.489.0",
    "no-kafka": "^3.1.10",
    "rsmq": "^0.12.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@types/chai": "4.2.17",
    "@types/mocha": "8.2.2",
    "@types/node": "12.20.12",
    "@types/rsmq": "0.8.4",
    "@types/sinon": "10.0.0",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-istanbul": "5.2.0",
    "chai": "4.3.4",
    "cross-env": "6.0.3",
    "eslint": "6.8.0",
    "eslint-config-ordermentum": "1.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-mocha": "8.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-unicorn": "31.0.0",
    "mocha": "8.4.0",
    "nyc": "14.1.1",
    "prettier": "1.19.1",
    "sinon": "10.0.0",
    "source-map-support": "0.5.19",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "typescript-eslint-parser": "22.0.0"
  }
}
